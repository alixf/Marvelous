draw (0,10)--(10,10)--(10,0)--((10+10)/3,(10+10)/3);

draw (0,10)--(10,10)--(10,0)--cycle;

draw (0,10)--+(10,0)--cycle--(10,0);

monImage = image
{
	draw (0,10)--(10,10)--(10,0)--((10+10)/3,(10+10)/3);
	draw (0,20)--(20,20)--(20,0)--((20+20)/3,(20+20)/3);
	draw (0,30)--(30,30)--(30,0)--((30+30)/3,(30+30)/3);
}

rotate(monImage, (0,0), 90);
translate(monImage, (0,0)--(15,15));

for(i = 0; i < 10; ++i)
{
	if(i % == 10)
	{
		draw (i,40+i)--(40+i,40+i)--(40+i,0+i)--((40+40)/3+i,(40+40)/3+i);
	}
}

function drawPeanuts()
{
	return image
	{
		draw (0,10)--(10,10)--(10,0)--((10+10)/3,(10+10)/3);
		draw (0,20)--(20,20)--(20,0)--((20+20)/3,(20+20)/3);
		draw (0,30)--(30,30)--(30,0)--((30+30)/3,(30+30)/3);
	}
}